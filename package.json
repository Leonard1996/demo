{
  "name": "reactjs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.20.7",
    "axios": "^0.26.1",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "react-scripts build && aws s3 rm s3://hyrabucket/ --recursive && aws s3 cp ./build s3://hyrabucket/ --recursive --acl public-read",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "plop --plopfile src/internals/generators/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals --fix --ext .js,.jsx .",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint . --ignore-path .gitignore --ignore-pattern internals --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "prettier": "^2.6.2"
  }
}
